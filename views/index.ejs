<html>
  <%- include("./partials/head.ejs") %>
  <body>
    <%- include("./partials/nav.ejs") %>
    <!-- ejs variable creating  -->
    <!-- <% const name="munees" %>  -->

    <div class="blogs content">
      <h2>All Blogs</h2>

      <% if(blogs.length >0){ %> <% blogs.forEach(blog=>{ %>
      <a class="single" href="/blogs/<%= blog._id %>">
        <h3><%= blog.title %></h3>
        <p><%= blog.snippet %></p>
      </a>
      <%}) %> <% } else { %>
      <p>No Blogs Available</p>
      <% }%>
    </div>
  </body>
</html>
